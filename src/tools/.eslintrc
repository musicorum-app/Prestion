{
  "env": {
      "commonjs": true,
      "es2021": true,
      "node": true
  },
  "extends": [
      "standard",
      "plugin:@typescript-eslint/recommended"
  ],
  "parserOptions": {
      "ecmaVersion": 11
  },
  "rules": {
      "camelcase": "off",
      "@typescript-eslint/explicit-module-boundary-types": "off"
  },
  "plugins": [
      "@typescript-eslint",
      "@emotion"
  ],
  "parser": "@typescript-eslint/parser"
}